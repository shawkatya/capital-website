RewriteEngine on

RewriteCond %{HTTP_REFERER} !^http://capitaldaycare.ca/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://capitaldaycare.ca$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.capitaldaycare.ca/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.capitaldaycare.ca$      [NC]
RewriteCond %{HTTP_REFERER} !^https://capitaldaycare.ca/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://capitaldaycare.ca$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.capitaldaycare.ca/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^https://www.capitaldaycare.ca$      [NC]
RewriteRule .*\.(jpg|jpeg|gif|png|bmp)$ - [F,NC]

